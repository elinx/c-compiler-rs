    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'kecc'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=kecc"
                ],
                "filter": {
                    "name": "kecc",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'kecc'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=kecc",
                    "--package=kecc"
                ],
                "filter": {
                    "name": "kecc",
                    "kind": "bin"
                }
            },
            "sourceMap": {
                "/rustc/49cae55760da0a43428eba73abcb659bb70cf2e4": "${env:HOME}/.rustup/toolchains/1.44.0-x86_64-unknown-linux-gnu/lib/rustlib/src/rust"
            },
